{
  "name": "homebridge-comelit-vedo-platform-custom-haas85",
  "version": "1.0.8",
  "author": "Pierpaolo Follia",
  "repository": {
    "type": "git",
    "url": "https://github.com/madchicken/homebridge-comelit-vedo"
  },
  "description": "Comelit Vedo support for Homebridge: https://github.com/nfarina/homebridge",
  "license": "Apache-2.0",
  "keywords": [
    "homebridge-plugin",
    "comelit",
    "vedo"
  ],
  "engines": {
    "node": ">8.15.1",
    "homebridge": ">1.1.0"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "src",
    "config.schema.json"
  ],
  "scripts": {
    "build": "yarn clean && yarn build-ts",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "yarn run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"src/*.{js,ts}\" --quiet --fix",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"{src,__{tests,mocks}__}/**/*.ts\"",
    "clean": "rimraf dist/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "express": "4.17.1",
    "lodash": "4.17.20",
    "prom-client": "^11.5.3",
    "typescript": "^3.9.7",
    "async-mqtt": "2.6.1",
    "axios": "0.19.2",
    "chalk": "3.0.0",
    "mqtt": "4.1.0",
    "mqtt-packet": "6.3.2",
    "querystring": "0.2.0",
    "yargs": "15.4.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/lodash": "^4.14.149",
    "@types/node": "^11.15.3",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "homebridge": "^1.1.1",
    "husky": "^4.2.3",
    "jest": "25.1.0",
    "nock": "^12.0.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "@types/chalk": "2.2.0",
    "@types/mqtt": "2.5.0",
    "@types/yargs": "15.0.5",
    "@types/jest": "25.1.4",
    "@types/nock": "11.1.0",
    "@types/axios": "0.14.0",
    "ts-jest": "25.2.1"
  }
}
